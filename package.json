{
    "name": "a11y-report-web",
    "version": "0.1.1",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/wildlyinaccurate/a11y-report-web"
    },
    "author": "Joseph Wynn <joseph@wildlyinaccurate.com>",
    "license": "MIT",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "prettier": {
        "endOfLine": "lf",
        "printWidth": 120,
        "semi": false
    },
    "dependencies": {
        "@azure/storage-queue": "^10.3.0",
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.7.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.1.1",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.0",
        "express-validator": "^6.2.0",
        "fbgraph": "^1.4.4",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "mongoose": "^5.7.7",
        "nodemailer": "^6.3.1",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.4",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.0.3",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.28",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.4",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/connect-mongo": "^0.0.43",
        "@types/dotenv": "^6.1.1",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.3",
        "@types/express": "^4.17.1",
        "@types/express-flash": "0.0.1",
        "@types/express-session": "^1.15.15",
        "@types/jest": "^24.0.21",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.144",
        "@types/lusca": "^1.6.1",
        "@types/mongoose": "^5.5.26",
        "@types/node": "^12.12.0",
        "@types/nodemailer": "^6.2.2",
        "@types/passport": "^1.0.1",
        "@types/passport-local": "^1.0.33",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.6.0",
        "@typescript-eslint/parser": "^2.6.0",
        "chai": "^4.2.0",
        "concurrently": "^4.1.2",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-prettier": "^3.1.1",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4",
        "prettier": "^1.18.2",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.4"
    }
}
